{
    "files.associations": {
        "iostream": "cpp"
    },
    "io-run.executorMap.common":{
    ".cpp": {
        "compileCmd": "g++ -DLOCAL -std=c++17 -Wshadow -Wall -fsanitize=address -fsanitize=undefined -D_GLIBCXX_DEBUG -g ${codeFile} -o ${codeFileNoExt}",
        "runCmd": "${dirCodeFileNoExt} <${inputFile} >${outputFile}",
        "cleanupCmd": "rm ${codeFileNoExt}",
        "errorTracer": "gdb"
      }
    }
}